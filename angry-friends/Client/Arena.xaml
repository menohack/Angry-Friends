<UserControl x:Class="Client.Arena"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Foreground="White">
    
    <Canvas x:Name="canvas" Height="308" Width="480">
        <TextBlock Canvas.Left="261" Canvas.Top="12" Height="284" Name="receiving" Text="Received:" Width="207" Foreground="Black" />
        <TextBlock Canvas.Left="12" Canvas.Top="39" Height="257" Name="sending" Text="Sent:" Width="243" Foreground="Black" TextWrapping="Wrap" />
        <Button Canvas.Left="12" Canvas.Top="12" Content="Send" Height="23" Name="button1" Width="75" />
        <Button Canvas.Left="93" Canvas.Top="12" Content="Receive" Height="23" Name="button2" Width="75" />
        <CheckBox Canvas.Left="174" Canvas.Top="15" Content="Test" Height="16" Name="test" />
        <Rectangle Canvas.Left="257" Canvas.Top="0" Height="308" Name="rectangle1" Stroke="{x:Null}" StrokeThickness="1" Width="2" Fill="#FF9A9A9A" />
    </Canvas>
</UserControl>
